{
  "$schema": "https://raw.githubusercontent.com/Azure/BatchAI/master/schemas/2017-09-01-preview/job.json",
  "properties": {
    "containerSettings": {
      "imageSourceRegistry": {
        "image": "masalvar/horovod-batchai-bench-intel:9-1.8-0.13.2"
      }
    },
    "customToolkitSettings": {
      "commandLine": "source /opt/intel/compilers_and_libraries_2017.4.196/linux/mpi/intel64/bin/mpivars.sh; echo $AZ_BATCH_HOST_LIST"
    },
    "nodeCount": 1,
    "stdOutErrPathPrefix": "$AZ_BATCHAI_MOUNT_ROOT/extfs"
  }
}

